SELECT ?battle ?battleLabel ?battleDescription ?start ?end ?coord
WHERE {
  VALUES ?rootConflict {
    wd:Q178561    # battle
    wd:Q200177    # military conflict
    wd:Q350604    # siege
    wd:Q1048835   # armed engagement
  }

  ?battle wdt:P31/wdt:P279* ?rootConflict.

  OPTIONAL { ?battle wdt:P580 ?start. }
  OPTIONAL { ?battle wdt:P582 ?end. }
  OPTIONAL { ?battle wdt:P625 ?coord. }

  SERVICE wikibase:label {
    bd:serviceParam wikibase:language "en".
  }
}
LIMIT 10000
